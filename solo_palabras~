#!/bin/bash

# crea solo palabras del log
in="$1"
canal="$2"
#filtro='[,:.\"\}\^\=\#\*\/\\]'
filtro=''
if [ ! -f "$in" ]; then
	echo "File not found! '$in'"
	echo "Usage: $0 <archivo_log>"
	exit
fi
echo 
while IFS= read -r line; do
	words=$(echo "$line" | grep "$canal" | awk '{$1=$2=$3=$4=$5=$6=""; print $0}')
	words=${words//$filtro}
	if [ "$words" ]; then
		echo -n "$words "
	fi
done < "$in"
echo;echo
